generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Price {
  id        Int      @id @default(autoincrement())
  token_address String
  value     Float
  timestamp DateTime @default(now())
}

model Alert {
  id        Int      @id @default(autoincrement())
  token_address     String
  target_price Float
  email      String
  createdAt  DateTime @default(now())
}